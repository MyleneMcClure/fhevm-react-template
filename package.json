{
  "name": "fhevm-sdk-project",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK with examples and documentation",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "setup": "npm install --workspaces",
    "build:sdk": "npm run build --workspace=packages/fhevm-sdk",
    "dev:nextjs": "npm run dev --workspace=examples/nextjs-showcase",
    "dev:museum": "npm run node --workspace=examples/museum-tracker",
    "deploy:museum": "npm run deploy --workspace=examples/museum-tracker"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "privacy",
    "blockchain",
    "ethereum",
    "sdk",
    "wagmi",
    "react",
    "nextjs"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/fhevm-sdk"
  }
}
